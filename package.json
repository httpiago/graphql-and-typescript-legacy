    "@types/faker": "^4.1.5",
    "@types/graphql": "^14.2.0",
    "@types/helmet": "^0.0.43",
    "@types/knex": "^0.16.1",
    "@types/node": "^11.13.8",
    "@types/nodemailer": "^4.6.8",
    "ts-node-dev": "^1.0.0-pre.32"
  },
  "engines": {
    "node": ">= 8.x"
  },
  "private": true
}
